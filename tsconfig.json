{
  "compilerOptions": {
    "paths": {
      "@*": ["./*"]
    },
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", ".next/types/**/*.ts", "**/*.ts", "**/*.tsx", "app/api/withPage/[query]/[page]/route.ts", "app/api/filmPage/[query]/[isMovie]/[id]/route.ts", "app/api/exact/[query]/route.ts"],
  "exclude": ["node_modules"]
}
